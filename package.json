{
  "name": "bestrong",
  "version": "1.0.0",
  "description": "Gym Management Application",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "prepare": "husky",
    "dev": "turbo run dev",
    "build": "turbo run build",
